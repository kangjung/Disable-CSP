{
    "name": "csp-disable",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "CSP DISABLE 프로그램",
    "homepage_url": "https://github.com/kangjung/Disable-CSP.git",
    "background": {
        "scripts": [ "background.js" ]
    },
    "browser_action": {
        "default_title": "CSP DISABLE"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "browsingData"
       
    ],
    "icons": { "16": "image/icon.png", "128": "image/icon.png" }
}